{
  "name": "dremio-ui",
  "private": true,
  "description": "Dremio",
  "repository": {
    "type": "git",
    "url": "git@github.com:dremio/dremio.git"
  },
  "browserslist": [
    "extends browserslist-config-dremio"
  ],
  "scripts": {
    "analyzeprod": "cross-env ENABLE_BUNDLE_ANALYZER=true npm run build",
    "build": "cross-env NODE_ENV=production webpack",
    "gen:nessie": "rimraf src/services/nessie/client && openapi-generator-cli generate -i http://localhost:19120/q/openapi -g typescript-fetch -o src/services/nessie/client --additional-properties=supportsES6=true,typescriptThreePlus=true,withoutRuntimeChecks=true",
    "lib:install": "node $EXTERNAL_LIB/libInstall.js",
    "licensecheck": "pnpm licenses list --json --prod | node ./node_modules/@dremio/oss-license-checker/dist/index.js",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "lint:js": "eslint --cache ./ ${DREMIO_UI_SRC:-src} --quiet",
    "lint:path": "eslint --cache",
    "overwrite-sourcemaps": "ts-node ./build-utils/overwriteSourcemaps.ts",
    "postinstall": "npm run licensecheck",
    "report:oss": "node licenseCheck.js",
    "start": "SKIP_SENTRY_STEP=true webpack serve",
    "test": "npm-run-all --print-label --parallel lint:js test:only ts-check",
    "test:base": "cd ../../.. && cross-env BABEL_ENV=test mocha --exit --require ${OLDPWD}/test/index.js --experimental-require-module",
    "test:coverage": "cross-env BABEL_ENV=test nyc mocha --require ./test/index.js ${DREMIO_UI_TESTS:-$PWD/src/\\**/*-spec.[t|j]s?(x)} --experimental-require-module",
    "test:coverage-html": "cross-env BABEL_ENV=test nyc mocha --require ./test/index.js ${DREMIO_UI_TESTS:-$PWD/src/\\**/*-spec.[t|j]s?(x)} && nyc report --reporter=html --experimental-require-module",
    "test:only": "npm run test:base -- ${DREMIO_UI_TESTS:-$PWD/src/\\**/*-spec.[t|j]s?(x)}",
    "test:single": "npm run test:base -- --watch",
    "test:watch": "npm run test:only -- --watch",
    "ts-check": "tsc --noEmit"
  },
