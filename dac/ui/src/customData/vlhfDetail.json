  "REDSHIFT": {
    "sourceType": "REDSHIFT", "tags":["beta"],
    "metadataRefresh": {"datasetDiscovery": true},
    "form": { "tabs": [
      {
        "name": "General",
        "isGeneral": true,
        "sections": [
          {
            "name": "Connection",
            "elements": [
              {
                "propName": "config.connectionString",
                "placeholder": "e.g. jdbc:redshift://123.123.123.123:5439/database",
